{
  "name": "CoachBridge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "android:device": "npx react-native run-android --device",
    "ios": "npx react-native run-ios",
    "ios:device": "npx react-native run-ios --device",
    "dev": "yarn start & yarn ios",
    "lint": "eslint --fix --ext .tsx --ext .ts src/",
    "prettier:fix": "prettier --write ./src",
    "prettier": "prettier --check ./src",
    "test": "jest --coverage --config ./jest.config.js",
    "clean": "watchman watch-del-all && rm -rf node_modules && yarn install",
    "reset": "rm -rf node_modules && cd ios && rm -rf Pods && pod install && cd .. && yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.1",
    "@react-native-vector-icons/ionicons": "^7.4.0",
    "@react-navigation/bottom-tabs": "^7.3.8",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "axios": "^1.8.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "react": "19.0.0",
    "react-native": "0.78.0",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-async-storage": "^0.0.1",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^3.17.0",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.10.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.11.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/eslint-config": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^9.21.0",
    "jest": "^29.6.3",
    "prettier": "^3.5.2",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
